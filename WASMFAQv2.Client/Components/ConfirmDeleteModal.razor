@inject IFAQApiService FAQApiService

<MudDialog>
    <DialogContent>
        <MudText Typo="Typo.h6">@AppStrings.DeleteConfirmation</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Default" OnClick="Cancel">@AppStrings.Cancel</MudButton>
        <MudButton Color="Color.Warning" OnClick="Submit">@AppStrings.Delete</MudButton>
    </DialogActions>
</MudDialog>

@code {


    [CascadingParameter]
    private IMudDialogInstance? MudDialog { get; set; }

    private void Submit()
    {
        MudDialog?.Close(DialogResult.Ok(true));
    }
    private void Cancel() => MudDialog?.Cancel();

}